	<Button name="SyncUI_CloseButtonTemplate" virtual="true">
		<Size x="17" y="17" />
		<NormalTexture file="Interface\FriendsFrame\ClearBroadcastIcon" alpha="0.5" />
		<HighlightTexture file="Interface\FriendsFrame\ClearBroadcastIcon" alpha="1" />
		<Scripts>
			<OnClick>
				local frame = self:GetParent()
				
				frame:Hide()
				
				if frame.fadeTimer or frame.forceFading then
					SyncUI_StopFrameFading(frame)
				end
			</OnClick>
		</Scripts>
	</Button>

	<EditBox name="SyncUI_EditBoxTemplate" autoFocus="false" letters="25" virtual="true">
		<Size x="120" y="19" />
		<FontString justifyH="LEFT" justivyV="CENTER" inherits="SyncUI_GameFontShadow_Medium" />
		<TextInsets left="5" right="20" top="0" bottom="0" />
		<Layers>
			<Layer level="ARTWORK">
				<FontString parentKey="Thumbnail" inherits="SyncUI_GameFontShadow_Medium" justifyH="LEFT">
					<Anchors>
						<Anchor point="TOPLEFT" x="5" y="0" />
						<Anchor point="BOTTOMRIGHT" x="-20" y="0" />
					</Anchors>
					<Color r="0.35" g="0.35" b="0.35"/>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<Frame parentKey="Backdrop" inherits="SyncUI_LayerWithNoEdgeTemplate" useParentLevel="true">
				<Anchors>
					<Anchor point="TOPLEFT" x="-8" y="8" />
					<Anchor point="BOTTOMRIGHT" x="8" y="-8" />
				</Anchors>
			</Frame>
			<Button parentKey="ClearButton" name="$parentClearButton" hidden="true">
				<Size x="17" y="17" />
				<Anchors>
					<Anchor point="RIGHT" x="0" y="0" />
				</Anchors>
				<Layers>
					<Layer level="ARTWORK">
						<Texture parentKey="texture" file="Interface\FriendsFrame\ClearBroadcastIcon" alpha="0.5" />
					</Layer>
				</Layers>
				<Scripts>
					<OnEnter>
						self.texture:SetAlpha(1.0)
						self:GetParent().ignoreFocusLoss = true;
					</OnEnter>
					<OnLeave>
						self.texture:SetAlpha(0.5)
						self:GetParent().ignoreFocusLoss = false;
					</OnLeave>
					<OnClick>
						self:GetParent():SetText("");
						self:GetParent():ClearFocus();
						self:GetParent().Thumbnail:Show();
						self:Hide();						
					</OnClick>
				</Scripts>
			</Button>
		</Frames>
		<Scripts>
			<OnLoad>
				self.Thumbnail:SetText(SYNCUI_STRING_ENTER_NAME)
			</OnLoad>
			<OnEditFocusGained>
				self.ClearButton:Show()
				self.Thumbnail:Hide()
			</OnEditFocusGained>
			<OnEditFocusLost>
				if self.ignoreFocusLoss then
					return
				end

				if (self:GetText() == "") then
					self.ClearButton:Hide();
					self.Thumbnail:Show()
				end
			</OnEditFocusLost>
			<OnEnterPressed>
				self:ClearFocus();
			</OnEnterPressed>
			<OnEscapePressed>
				self:ClearFocus();
			</OnEscapePressed>
			<OnEnter>
				self.ignoreFocusLoss = true;
			</OnEnter>
			<OnLeave>
				self.ignoreFocusLoss = false;
			</OnLeave>
			<OnHide>
				self:SetText("");
				self.Thumbnail:Show();
				self.ClearButton:Hide();
			</OnHide>
		</Scripts>
		
	</EditBox>

