					<Layer level="ARTWORK">
						<Texture name="$parentFill" inherits="TotalAbsorbBarTemplate" />
					</Layer>
					<Layer level="ARTWORK" textureSubLevel="1">
						<Texture name="$parentOverlay" inherits="TotalAbsorbBarOverlayTemplate" />
					</Layer>
				</Layers>
			</Frame>
