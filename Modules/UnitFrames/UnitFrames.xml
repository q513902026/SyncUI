					<Layer level="BACKGROUND">
						<Texture file="Interface\AddOns\SyncUI\Media\Textures\UnitFrames\UnitFrame-Normal">
							<TexCoords left="0" right="0.8984375" top="0" bottom="0.8125" />
						</Texture>
