					<OnLoad>
						SyncUI_DisableFrame("CastingBarFrame")
						SyncUI_RegisterDragFrame(self, SYNCUI_STRING_PLACEMENT_TOOL_LABEL_CASTBAR, nil, true)
						SyncUI_CastingBar_OnLoad(self)
